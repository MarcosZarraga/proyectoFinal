{{outlet}}

<div class="row">
	<div class="col s2">
		{{volver-main}}
    <div class="fixed-action-btn">
      <button class="btn-floating btn-large "{{action 'goToNuevaArea'}}> Crear</button>
    </div>
		
	</div>
	<div class="col s10">
		{{#each model as |area|}}
			<div class="row">
        		<div class="col s12 m6">
          			<div class="card blue-grey darken-1">
            			<div class="card-content white-text">
              				<span class="card-title">{{area.name}}</span>
              				{{area.info}}
            			</div>
            			<div class="card-action">
              				{{area.ubication}}
            			</div>
          			</div>
        		</div>
     	 	</div>
		{{/each}}
	</div>
</div>
